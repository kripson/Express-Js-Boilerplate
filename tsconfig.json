{
    "compilerOptions": {
        "module": "CommonJS",
        "outDir": "dist",
        "target": "es2020",
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "experimentalDecorators": true
    }
}